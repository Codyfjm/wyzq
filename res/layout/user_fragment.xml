<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/other_image_width"
        android:background="@drawable/other_frame"
        android:orientation="vertical"
        android:paddingLeft="@dimen/other_left"
        android:paddingRight="@dimen/other_left" >

        <RelativeLayout
            android:id="@+id/other_user"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/login_height" >

            <ImageView
                android:id="@+id/title1"
                android:layout_width="@dimen/other_image_width"
                android:layout_height="@dimen/other_image_width"
                android:layout_centerVertical="true"
                android:scaleType="fitCenter"
                android:src="@drawable/ios_contact_outline" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="3dp"
                android:layout_toRightOf="@id/title1"
                android:text="个人信息"
                android:textColor="#565656" />

            <ImageView
                android:layout_width="@dimen/other_image_width2"
                android:layout_height="@dimen/other_image_width2"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/ios_arrow_forward" />
        </RelativeLayout>

        <View
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="#E8E8E8" />

        <RelativeLayout
            android:id="@+id/update_paw"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/login_height" >

            <ImageView
                android:id="@+id/title2"
                android:layout_width="@dimen/other_image_width"
                android:layout_height="@dimen/other_image_width"
                android:layout_centerVertical="true"
                android:scaleType="fitCenter"
                android:src="@drawable/compose" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="3dp"
                android:layout_toRightOf="@id/title2"
                android:text="修改密码"
                android:textColor="#565656" />

            <ImageView
                android:layout_width="@dimen/other_image_width2"
                android:layout_height="@dimen/other_image_width2"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/ios_arrow_forward" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/other_material"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/login_height"
            android:visibility="gone" >

            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="#E8E8E8" />

            <ImageView
                android:id="@+id/title5"
                android:layout_width="@dimen/other_image_width"
                android:layout_height="@dimen/other_image_width"
                android:layout_centerVertical="true"
                android:scaleType="fitCenter"
                android:src="@drawable/link" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="3dp"
                android:layout_toRightOf="@id/title5"
                android:text="房下账号"
                android:textColor="#565656" />

            <ImageView
                android:layout_width="@dimen/other_image_width2"
                android:layout_height="@dimen/other_image_width2"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/ios_arrow_forward" />
        </RelativeLayout>

        <View
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="#E8E8E8" />

        <RelativeLayout
            android:id="@+id/update_share"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/login_height" >

            <ImageView
                android:id="@+id/title7"
                android:layout_width="@dimen/other_image_width"
                android:layout_height="@dimen/other_image_width"
                android:layout_centerVertical="true"
                android:scaleType="fitCenter"
                android:src="@drawable/at" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="3dp"
                android:layout_toRightOf="@id/title7"
                android:text="我要分享"
                android:textColor="#565656" />

            <ImageView
                android:layout_width="@dimen/other_image_width2"
                android:layout_height="@dimen/other_image_width2"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/ios_arrow_forward" />
        </RelativeLayout>

        <View
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="#E8E8E8" />

        <RelativeLayout
            android:id="@+id/update_say"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/login_height" >

            <ImageView
                android:id="@+id/title8"
                android:layout_width="@dimen/other_image_width"
                android:layout_height="@dimen/other_image_width"
                android:layout_centerVertical="true"
                android:scaleType="fitCenter"
                android:src="@drawable/chatbubble" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="3dp"
                android:layout_toRightOf="@id/title8"
                android:text="我的说说"
                android:textColor="#565656" />

            <ImageView
                android:layout_width="@dimen/other_image_width2"
                android:layout_height="@dimen/other_image_width2"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/ios_arrow_forward" />
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/other_image_width"
        android:background="@drawable/other_frame"
        android:orientation="vertical"
        android:paddingLeft="@dimen/other_left"
        android:paddingRight="@dimen/other_left" >

        <RelativeLayout
            android:id="@+id/other_update"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/login_height" >

            <ImageView
                android:id="@+id/title4"
                android:layout_width="@dimen/other_image_width"
                android:layout_height="@dimen/other_image_width"
                android:layout_centerVertical="true"
                android:scaleType="fitCenter"
                android:src="@drawable/cloud" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="3dp"
                android:layout_toRightOf="@id/title4"
                android:text="检查更新"
                android:textColor="#565656" />

            <ImageView
                android:layout_width="@dimen/other_image_width2"
                android:layout_height="@dimen/other_image_width2"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/ios_arrow_forward" />
        </RelativeLayout>

        <View
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="#E8E8E8" />

        <RelativeLayout
            android:id="@+id/other_exit"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/login_height" >

            <ImageView
                android:id="@+id/title6"
                android:layout_width="@dimen/other_image_width"
                android:layout_height="@dimen/other_image_width"
                android:layout_centerVertical="true"
                android:scaleType="fitCenter"
                android:src="@drawable/ios7_redo" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="3dp"
                android:layout_toRightOf="@id/title6"
                android:text="退出登陆"
                android:textColor="#565656" />

            <ImageView
                android:layout_width="@dimen/other_image_width2"
                android:layout_height="@dimen/other_image_width2"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/ios_arrow_forward" />
        </RelativeLayout>
    </LinearLayout>

</LinearLayout>